{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Nautilus",
  "version": "1.0.0",
  "identifier": "nti.nautilus",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5174",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Nautilus",
        "width": 800,
        "height": 600,
        "center": true,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "visible": false,
        "url": "index.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "binaries/*": "binaries/"
    }
  }
}