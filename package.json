{
  "name": "nautilus",
  "version": "1.0.0",
  "description": "Simple Server Manager application built with Tauri.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:backend": "cd backend && npm start",
