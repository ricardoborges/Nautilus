    "tauri:dev": "npm run build:backend && tauri dev",
    "tauri:build": "npm run build:backend && tauri build",
    "build:backend": "cd backend && npm install && npm run build"
  },
  "overrides": {
    "cpu-features": "npm:empty-npm-package@1.0.0"
